    "list.focusBackground": "#F2EAD4",
    "list.focusForeground": "#606C6C",
    "list.focusOutline": "#3888B8",
    "list.hoverBackground": "#F7F0DD",
    "list.highlightForeground": "#3888B8",

    "textLink.foreground": "#3888B8",
    "textLink.activeForeground": "#309890",
    "badge.background": "#3888B8",
