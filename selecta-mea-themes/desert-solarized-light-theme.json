    "terminal.ansiBrightYellow": "#B85830",
    "terminal.ansiBrightBlue": "#3888B8",
    "terminal.ansiBrightMagenta": "#7870A8",
    "terminal.ansiBrightCyan": "#309890",
    "terminal.ansiBrightWhite": "#606C6C"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {"foreground": "#9AA5A5"}
    },
    {
      "scope": ["constant", "support.constant"],
      "settings": {"foreground": "#B85830"}
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {"foreground": "#7070A8"}
    },
    {
      "scope": "entity.name.class",
      "settings": {"foreground": "#7070A8"}
    },
    {
      "scope": "entity.name.function",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "entity.name.section",
      "settings": {"foreground": "#B85830"}
    },
    {
      "scope": "entity.name.tag",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": ["keyword", "keyword.operator.expression", "keyword.operator.new"],
      "settings": {"foreground": "#7870A8"}
    },
    {
      "scope": "keyword.control.at-rule, keyword.control.content",
      "settings": {"foreground": "#309890"}
    },
    {
      "scope": "keyword.function",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "keyword.operator",
      "settings": {"foreground": "#606C6C"}
    },
    {
      "scope": "keyword.other.unit",
      "settings": {"foreground": "#B85830"}
    },
    {
      "scope": "markup.bold",
      "settings": {"fontStyle": "bold", "foreground": "#606C6C"}
    },
    {
      "scope": "markup.fenced_code.block",
      "settings": {"foreground": "#7C8888"}
    },
    {
      "scope": "markup.inline.raw.string",
      "settings": {"foreground": "#889028"}
    },
    {
      "scope": "meta.link",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "meta.brace.square",
      "settings": {"foreground": "#606C6C"}
    },
    {
      "scope": ["entity.name.function.method", "markup.heading", "meta.definition.method"],
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "meta.object-literal.key",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "meta.tag.attributes",
      "settings": {"foreground": "#7070A8"}
    },
    {
      "scope": "meta.tag.sgml.doctype",
      "settings": {"foreground": "#7C8888"}
    },
    {
      "scope": "meta.type.annotation",
      "settings": {"foreground": "#7070A8"}
    },
    {
      "scope": "punctuation.definition.template-expression",
      "settings": {"foreground": "#309890"}
    },
    {
      "scope": "punctuation.definition.block",
      "settings": {"foreground": "#606C6C"}
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "storage",
      "settings": {"foreground": "#7870A8"}
    },
    {
      "scope": "storage.type.function",
      "settings": {"foreground": "#7870A8"}
    },
    {
      "scope": "string",
      "settings": {"foreground": "#889028"}
    },
    {
      "scope": "support",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "support.type.property-name",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "variable.language.this",
      "settings": {"foreground": "#309890"}
    },
    {
      "scope": "text",
      "settings": {"foreground": "#606C6C"}
    },
    {
      "scope": "meta.diff.header",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "meta.diff.range",
      "settings": {"foreground": "#309890"}
    },
    {
      "scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.parent-selector-suffix.css",
      "settings": {"foreground": "#606C6C"}
    },
    {
      "scope": "markup.deleted.diff",
      "settings": {"foreground": "#B85830"}
    },
    {
      "scope": "markup.inserted.diff",
      "settings": {"foreground": "#889028"}
    },
    {
      "scope": "markup.changed.diff",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "support.type.class.flowtype",
      "settings": {"foreground": "#7070A8"}
    },
    {
      "scope": "punctuation.definition.block.tag.jsdoc",
      "settings": {"foreground": "#309890"}
    },
    {
      "scope": "storage.type.class.jsdoc",
      "settings": {"foreground": "#7070A8"}
    },
    {
      "scope": "variable.other.jsdoc",
      "settings": {"foreground": "#606C6C"}
    },
    {
      "scope": "entity.name.import.go",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "markup.underline.link",
      "settings": {"foreground": "#3888B8"}
    },
    {
      "scope": "keyword.other.important.scss",
      "settings": {"foreground": "#B85830"}
    },
    {
      "scope": "variable.interpolation.scss",
      "settings": {"foreground": "#309890"}
    },
    {
      "scope": "variable.scss",
      "settings": {"foreground": "#3888B8"}
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#606C6C",
    "function": {"foreground": "#3888B8"},
    "method": {"foreground": "#3888B8"},
    "class": {"foreground": "#7070A8"},
    "type": {"foreground": "#7070A8"},
    "property": {"foreground": "#3888B8"},
    "parameter": {"foreground": "#309890"}
  }
}
