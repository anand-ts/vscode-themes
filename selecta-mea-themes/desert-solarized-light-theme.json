    "editorSuggestWidget.foreground": "#606C6C",
    "editorSuggestWidget.border": "#E6DDC5",
    "editorSuggestWidget.selectedBackground": "#F2EAD4",
    "editorSuggestWidget.highlightForeground": "#3888B8",

    "quickInput.background": "#F7F0DD",
    "quickInput.foreground": "#606C6C",
    "quickInputList.focusBackground": "#F2EAD4",
    "quickInputList.focusForeground": "#606C6C",
    "input.background": "#FCF6E4",
    "input.foreground": "#606C6C",
    "input.border": "#E6DDC5",
